{
    "manifestVersion": 1,
    "id": "azp-utilities-boots",
    "name": "Boots & Utilities",
    "version": "0.1.0",
    "publisher": "pjcollins",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Custom tasks for Azure Pipelines. Featuring Boots!",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "Images/boots.png"
    },
    "files": [
        {
            "path": "Boots"
        }
    ],
    "contributions": [
        {
            "id": "boots",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Boots"
            }
        }
    ]
}
